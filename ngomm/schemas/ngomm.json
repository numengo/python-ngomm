{
  "$id": "https://numengo.org/ngomm#",
  "type": "object",
  "$comment": "automatically generated for ngomm",
  "$defs": {
    "contexts": {
      "type": "object",
      "$defs": {
        "NodeContext": {
          "type": "object",
          "notSerialized": [
            "_parentObjectNode"
          ],
          "notValidated": [
            "_parentObjectNode"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/contexts/$defs/InstanceContext"
          ],
          "properties": {
            "_parentObjectNode": true
          }
        }
      }
    },
    "serializers": {
      "type": "object",
      "$defs": {
        "NodeSerializer": {
          "type": "object",
          "notSerialized": [
            "untypedNodes",
            "iconInstanceNode"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/protocols/$defs/Serializer"
          ],
          "properties": {
            "node": {
              "$ref": "https://numengo.org/freeplane#/$defs/Node"
            },
            "untypedNodes": {
              "type": "array",
              "items": {
                "$ref": "https://numengo.org/freeplane#/$defs/Node"
              }
            },
            "iconInstanceNode": {
              "type": "string"
            }
          }
        }
      }
    },
    "mixins": {
      "type": "object",
      "$defs": {
        "AbstractNode": {
          "type": "object",
          "abstract": true,
          "extends": [
            "#/$defs/contexts/$defs/NodeContext",
            "#/$defs/serializers/$defs/NodeSerializer"
          ]
        },
        "TranslatedNode": {
          "type": "object",
          "abstract": true,
          "properties": {
            "source_id": {
              "type": "string",
              "node ID the corresponding source translation": {
                "_icons": [
                  "idea"
                ]
              }
            },
            "language": {
              "type": "string"
            }
          }
        },
        "HasFile": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/files/$defs/File"
          ],
          "properties": {
            "file_document": {
              "$ref": "https://numengo.org/ngoschema#/$defs/files/$defs/Document"
            }
          }
        },
        "HasFolder": {
          "type": "object",
          "properties": {
            "folder": {
              "type": "path"
            }
          }
        },
        "HasImage": {
          "type": "object",
          "extends": [
            "#/$defs/mixins/$defs/HasFile"
          ],
          "properties": {
            "image_type": {
              "type": "string"
            },
            "file_document": {
              "$ref": "https://numengo.org/ngoschema#/$defs/files/$defs/Document"
            }
          }
        },
        "HasVideo": {
          "type": "object",
          "extends": [
            "#/$defs/mixins/$defs/HasFile"
          ],
          "properties": {
            "video_type": {
              "type": "string"
            }
          }
        },
        "HasLink": {
          "type": "object",
          "properties": {
            "url": {
              "type": "uri"
            }
          }
        },
        "HasDescription": {
          "type": "object",
          "extends": [
            "#/$defs/mixins/$defs/AbstractNode"
          ],
          "properties": {
            "description": {
              "type": "string"
            }
          }
        }
      }
    },
    "instances": {
      "type": "object",
      "$defs": {
        "InstanceNode": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Instance",
            "#/$defs/mixins/$defs/AbstractNode"
          ]
        },
        "EntityNode": {
          "type": "object",
          "dependencies": {
            "node_id": [
              "node"
            ]
          },
          "primaryKeys": [
            "node_id"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity",
            "#/$defs/mixins/$defs/AbstractNode"
          ],
          "properties": {
            "node_id": {
              "type": "string"
            },
            "last_modified": {
              "type": "string"
            }
          }
        }
      }
    },
    "repositories": {
      "type": "object",
      "$defs": {
        "InstanceNodeRepository": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/repositories/$defs/MemoryRepository"
          ],
          "instanceClass": "ngomm.models.instances.InstanceNode"
        },
        "EntityNodeRepository": {
          "type": "object",
          "extends": [
            "#/$defs/repositories/$defs/InstanceNodeRepository"
          ],
          "instanceClass": "ngomm.models.instances.EntityNode"
        }
      }
    }
  }
}